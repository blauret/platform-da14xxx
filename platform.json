{
    "name": "da14xxx",
    "title": "Dialog Smartbond bluetooth microcontrollers",
    "description": "Dialog Smartbond bluetooth microcontrollers. Based on ARM Cortex-M family with bluetooth low energy complete stack",
    "url": "https://www.dialog-semiconductor.com/bluetooth-low-energy",
    "homepage": "https://platformio.org/platforms/da14xxx",
    "license": "Apache-2.0",
    "engines": {
        "platformio": "~4.0.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/blauret/platform-da14xxx.git"
    },
    "version": "0.0.0",
    "packageRepositories": [
        "https://dl.bintray.com/platformio/dl-packages/manifest.json",
        "http://dl.platformio.org/packages/manifest.json"
      ],
    "frameworks": {
        "cmsis": {
            "package": "framework-cmsis",
            "script": "builder/frameworks/cmsis.py"
        },
        "libopencm3": {
            "package": "framework-libopencm3",
            "script": "builder/frameworks/libopencm3/libopencm3.py"
        }
    },
    "packages": {
        "toolchain-gccarmnoneeabi": {
            "type": "toolchain",
            "version": ">=1.60301.0,<1.80000.0"
        },
        "framework-cmsis": {
            "type": "framework",
            "optional": true,
            "version": "~2.50400.181126"
        },
        "framework-libopencm3": {
            "type": "framework",
            "optional": true,
            "version": "~1.1.0"
        },
        "tool-openocd": {
            "type": "uploader",
            "optional": true,
            "version": "~2.1000.0"
        },
        "tool-jlink": {
            "type": "uploader",
            "optional": true,
            "version": "^1.63208.0"
        }
    }
}